
<h1>CANDIDATE LIST</h1>
<% @Applicants.each do |applicant| %>
  <div class="job">   
    <h3><%#= job.company %></h3>
    <h3><%= link_to applicant.name, applicant.lastname %></h3>
    <%# <h3><%= applicant.lastname %>
    <h3><%= applicant.city %></h3>
    <h3><%= applicant.phone %></h3>
    
    
    <p><%= button_to "Accept", job_accept_path(applicant), method: :patch %></p>
    <%= button_to "Reject", job_decline_path(applicant), method: :patch %> 
 <%= link_to "Edit", edit_job_path(applicant)%>
  </div>
<% end %>

<p><%= button_to "Sign out", destroy_user_session_path, method: :delete %>
